{
  "extends": "google",
  "rules": {
    "no-multi-spaces": [1, {
      "exceptions": {
        "VariableDeclarator": true,
        "ImportDeclaration": true,
        "AssignmentExpression": true
      }
    }],
    "key-spacing": [2, {
      "align": "value"
    }],
    "max-nested-callbacks": [2,8]
  },
  "env": {
    "browser": true,
    "mocha": true
  }
}
